<polylineencoder>
  <status code="{statusCode}">{statusMessage}</status>
  <points count="{pointCount}">
    <encoded>{encodedPointsLiteral}</encoded>
    <levels>{encodedLevels}</levels>
  </points>
  <bounds>
    <lat min="{minLat}" max="{maxLat}"/>
    <lng min="{minLng}" max="{maxLng}"/>
    <alt min="{minAlt}" max="{maxAlt}"/>
  </bounds>
  <center lat="{centerLat}" lng="{centerLng}"/>
  <created timestamp="{createdDate}"/>
</polylineencoder>